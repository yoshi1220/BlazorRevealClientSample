@inject NavigationManager Navigation

<h3>ログイン画面に移動します。</h3>

@code {

    protected override void OnAfterRender(bool firstRender)
    {
        try
        {
            Navigation.NavigateTo("Identity/Account/Login", true);
        }
        catch (Exception e)
        {
            Console.WriteLine(e.ToString());
            throw;
        }
    }

}